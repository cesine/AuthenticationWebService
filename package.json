  ],
  "dependencies": {
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "config": "^3.0.0",
    "cors": "^2.8.5",
    "cradle": "^0.7.1",
    "debug": "^4.1.0",
    "express": "^4.17.1",
    "express-bunyan-logger": "^1.3.3",
    "express-oauth-server": "^2.0.0",
    "fielddb": "https://github.com/fielddb/fielddb.git#v5.121.19-rc0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "nano": "^7.1.1",
    "nodemailer": "^4.7.0",
    "passport": "^0.3.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "q": "^1.5.1",
    "sequelize": "^3.23.1",
