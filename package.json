  "description": "Authentication web services for FieldDB.",
  "homepage": "https://github.com/FieldDB/FieldDB/issues/milestones?state=closed",
  "repository": {
    "type": "git",
    "url": "git://github.com/FieldDB/AuthenticationWebService.git"
